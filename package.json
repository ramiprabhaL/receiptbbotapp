{
  "name": "receipt-bot-app",
  "version": "1.0.0",
  "description": "AI-based expense tracker that helps users manage their finances by automatically processing and categorizing receipts",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "npm run start:backend",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "clean": "rimraf node_modules frontend/node_modules backend/node_modules",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "receipt",
    "expense-tracker",
    "ai",
    "categorization",
    "finance",
    "nextjs",
    "nodejs",
    "react"
  ],
  "author": "",
  "license": "MIT"
}
